sup   saber  irr_prt    [tam=[+prt]]
# not for 1s pres
sep   saber  e_sub_prs1 [tam=[+sub,+tmp,-imv,-prt,-fut,-ipf]];[tam=[-sub,-tmp,+imv,-prt,-fut,-ipf]]
sabr' saber   fut       [tam=[+fut,-prt,-sub,-imv,+tmp]]
sé    saber  end        [tam=[-prt,-fut],sj=[+1,-2,-p]]
# SALIR
