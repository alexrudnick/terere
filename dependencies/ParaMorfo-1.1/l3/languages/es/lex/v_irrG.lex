# pret regular
sal   salir   ie_prt     [tam=[+prt,+tmp]]
# SER
# future, conditional
ser'  ser   fut         [tam=[+fut,-prt,-sub,-imv,+tmp]]
# present
soy   ser   end         [tam=[-prt,-fut,+tmp],sj=[+1,-2,-p]]
#eres  ser   end         [tam=[-prt,-fut,+tmp],sj=[-1,+2,-p]]
# vos
sos   ser   end         [tam=[-prt,-fut,+tmp],sj=[-1,+2,-p]]
es    ser   end         [tam=[-prt,-fut,+tmp],sj=[-1,-2,-p]]
somos ser   end         [tam=[-prt,-fut,+tmp],sj=[+1,+p]]
son   ser   end         [tam=[-prt,-fut,+tmp],sj=[-1,+p]]
# present subjunctive
se    ser   e_sub_prs1  [tam=[+sub,+tmp,-imv,-prt,-fut,-ipf]];[tam=[-sub,-tmp,+imv,-prt,-fut,-ipf]]
# preterite, past subjunctive
fue  ser    end         [tam=[+prt,-fut,-sub,+tmp],sj=[-1,-2,-p]]
fui  ser    end         [tam=[+prt,-fut,-sub,+tmp],sj=[+1,-2,-p]]
fui  ser    prt2        [tam=[+prt,-fut,-sub,+tmp],sj=[+2,-p]] ; [tam=[+prt,-fut,-sub,+tmp],sj=[+1,+p]]
fue  ser    prt2        [tam=[+prt,-fut,+tmp],sj=[-1,+p]] ; [tam=[+prt,-fut,+sub,+tmp]]
# infinitive, gerund, participle
s    ser   stem_e1      [tam=[-tmp,+part]] ; [tam=[-tmp,+ger]]; [tam=[-tmp,+inf]]
# imperfect
'era  ser   person       [tam=[+ipf,-imv,+tmp]]
# imperative
sé   ser   postpron     [tam=[-tmp,+imv],sj=[-1,+2,-p]]
# VALER
val    valer  stem_i1    [tam=[-prt,-fut]]
valdr' valer  fut        [tam=[+fut,-prt,-sub,-imv,+tmp]]
valg   valer  e_sub_prs1 [tam=[-prt,-fut,-ipf,-cnt,-prf]]
# pret regular
val    valer  ie_prt     [tam=[+prt,+tmp]]
